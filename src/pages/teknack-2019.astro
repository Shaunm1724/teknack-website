---
import Header from "../components/Header.astro";
import Preloader from "../components/Preloader.astro";
import Navbar from "../components/Navbar.astro";
import InnerPageHero from "../components/InnerPageHero.astro";
import Footer from "../components/Footer.astro";

const title = "2019 Games";
---

<!doctype html>
<html lang="en">
  <head>
    <Header title={title} />
  </head>

  <body>
    <Preloader />
    <Navbar />
    <InnerPageHero title={title} />

    <!-- Portfolio Section Begin -->
    <section class="portfolio spad">
      <div class="container">
        <div class="row"></div>
        <div class="row portfolio__gallery">
          <div class="col-lg-3 col-md-3 col-sm-6 mix casual">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/GetMeOut.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.GetMeOut"
                  target="_blank"
                >
                  <h4>Get Me Out</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.GetMeOut"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 mix digital-marketing">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/UpsideDown.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.UpSideDown"
                  target="_blank"
                >
                  <h4>Upside Down</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.UpSideDown"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 mix web">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/CoconutRescue.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.CoconutRescue"
                  target="_blank"
                >
                  <h4>Coconut Rescue</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.CoconutRescue"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 mix photography">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/BattleTank.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.BattleTank"
                  target="_blank"
                >
                  <h4>Battle Tank</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.BattleTank"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 mix ecommerce">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/SpaceBreaker.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.SpaceBreaker"
                  target="_blank"
                >
                  <h4>Space Breaker</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.SpaceBreaker"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 mix casual">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/Survival.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.Survival"
                  target="_blank"
                >
                  <h4>Survival</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.Survival"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>

          <div class="col-lg-3 col-md-3 col-sm-6 mix casual">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/StackIt.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.StackIT"
                  target="_blank"
                >
                  <h4>Stack It</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.StackIT"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 mix digital-marketing">
            <div class="portfolio__item">
              <div
                class="portfolio__item__video set-bg"
                data-setbg="img/game-logos/OneFIneNight.png"
              >
              </div>
              <div class="portfolio__item__text">
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.OneFineNight"
                  target="_blank"
                >
                  <h4>One Fine Night</h4>
                </a>
                <a
                  href="https://play.google.com/store/apps/details?id=com.Teknack.OneFineNight"
                  target="_blank"><span>Download</span></a
                >
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <Footer />
  </body>
</html>
